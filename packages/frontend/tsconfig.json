{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~assets/*": ["src/assets/*"],
			"~modules/*": ["src/modules/*"],
			"~router/*": ["src/router/*"],
			"~shared/*": ["src/shared/*"],
			"~store/*": ["src/store/*"],
			"~typings/*": ["src/typings/*"],
			"~/*": ["src/*"],
			"~app/*": ["src/modules/app/*"],
			"~yup.schemas/*": ["src/shared/yup.schemas/*"],
			"~services/*": ["src/shared/services/*"]
		}
	}
}
